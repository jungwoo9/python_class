def diamond(n):
    for i in range(1, n):
        lst = []
        for a in range(1, n-i):
            lst.append(" ")
        for b in range(1, n):
            lst.append("* ")
        print(lst)
    for i in range(n, 1, -1):
        lst = []
        for a in range(1, n-i):
            lst.append(" ")
        for b in range(1, n):
            lst.append("* ")
        print(lst)